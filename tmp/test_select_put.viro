; Test select and put natives

; Create test object
obj: object [x: 10 y: 20 z: 30]

; Test select
print select obj 'x
print select obj 'y

; Test select with --default for missing field
print select obj 'missing --default 999

; Test put
put obj 'x 42
print select obj 'x

; Test put with another field
put obj 'y 100
print select obj 'y

; Test nested objects
parent: object [a: 1 b: 2]
child: make parent [c: 3]

; Select from child (should find in child and parent)
print select child 'c
print select child 'a

; Put in child
put child 'c 99
print select child 'c

quit
