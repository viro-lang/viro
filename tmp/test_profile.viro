factorial: fn [n] [
    if (= n 0) [
        1
    ] [
        * n (factorial (- n 1))
    ]
]

result1: factorial 5
result2: factorial 10
result3: factorial 7

print result1
print result2
print result3

sum: fn [a b] [+ a b]
product: fn [a b] [* a b]

x: sum 10 20
y: product 5 6
z: sum x y

print x
print y
print z
