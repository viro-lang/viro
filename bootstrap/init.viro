
import: fn [file] [ do parse tokenize read file ]

map: fn [series f] [
  result: []
  foreach series item [
    append result f item
  ]
  result
]

any: fn [conditions] [
  while [not tail? conditions] [
    value: do --next 'conditions conditions
    when value [ return true ]
  ]
  false
]

all: fn [conditions] [
  while [not tail? conditions] [
    value: do --next 'conditions conditions
    when not value [ return false ]
  ]
  return true
]

